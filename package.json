{
  "name": "record-app",
  "version": "2.0.0-beta.12",
  "private": true,
  "scripts": {
    "postinstall": "prisma generate",
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "seed": "node prisma/seed.mjs",
    "test": "vitest run --exclude __tests__/integration/**",
    "test:integration": "RUN_INTEGRATION_TESTS=true vitest run __tests__/integration/",
    "test:all": "RUN_INTEGRATION_TESTS=true vitest run",
    "test:watch": "vitest --exclude __tests__/integration/**",
    "test:coverage": "vitest run --coverage --exclude __tests__/integration/**"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@prisma/client": "^6.2.1",
    "@supabase/supabase-js": "^2.95.3",
    "@upstash/ratelimit": "^2.0.8",
    "@upstash/redis": "^1.36.2",
    "next": "15.5.11",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "dotenv": "^16.4.7",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.0.0",
    "prisma": "^6.2.1",
    "@vitest/coverage-v8": "^2.1.8",
    "vitest": "^2.1.8"
  }
}
